@using TravelTripProject.Models.Sınıflar
@model List<Blog>

<div class="box1-left">
    @foreach (var item in Model)
    {
<div class="bot-img">
    <img src="@item.BlogImage" style="width:150px; height:75px;" alt="" />
</div>
    <div class="bot-desc1">
        <p>@item.Aciklama.Substring(0,75) </p>
    </div>
    <div class="clear"></div>
    }
</div>


